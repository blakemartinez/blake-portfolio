---
import Footer from '../components/Footer.astro';
import Header from '../components/Header.astro';
import MyKeebs from '../components/MyKeebs.astro';
import "../styles/global.css"
---
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <link rel="apple-touch-icon" sizes="180x180" href="/favicon/apple-touch-icon.png">
<link rel="icon" type="image/png" sizes="32x32" href="/favicon/favicon-32x32.png">
<link rel="icon" type="image/png" sizes="16x16" href="/favicon/favicon-16x16.png">
<link rel="manifest" href="/favicon/site.webmanifest">
<link rel="mask-icon" href="/favicon/safari-pinned-tab.svg" color="#5bbad5">
<meta name="msapplication-TileColor" content="#da532c">
<meta name="theme-color" content="#b8a7aa">
<meta name="viewport" content="width=device-width" />
    <meta name="generator" content={Astro.generator} />
    <title>keebs</title>
  </head>
  <body>
    <Header/>
<!-- <MyKeebs> -->
  <div style="align-items: center; text-align: center; ">

    <model-viewer       style="width: 100%; height: 500px; top: 0px;"
    src="../keebRender.gltf" ar ar-modes="webxr scene-viewer quick-look" camera-controls poster="poster.webp" camera-orbit="-90.41deg 61.51deg 0.8191m" field-of-view="30deg" shadow-intensity="1" auto-rotate> </model-viewer>
  </div>
      <Footer />
    <script>
      import "../scripts/menu.js";
    </script>
    <script type="module" src="https://ajax.googleapis.com/ajax/libs/model-viewer/3.1.1/model-viewer.min.js"></script>
  </body>
</html>